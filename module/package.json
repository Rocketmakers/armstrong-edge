{
  "name": "@rocketmakers/armstrong-edge",
  "version": "0.0.1",
  "description": "Rocketmakers front-end library for parsing a generated Typescript API client into a set of configurable React hooks for fetching and mutating data. Currently set as a private module to prevent publishing",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "copy-readme": "shx cp ../README.md .",
    "build-ts": "tsc --pretty --extendedDiagnostics -p src -d",
    "build": "npm run build-ts && npm run build-sass && npm run copy-readme",
    "link": "npm link",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --max-warnings=0 \"**/*.ts{,?}\" --cache --ignore-path .eslintignore --ext ts --ext tsx",
    "lint-fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run build",
    "build-sass": "cd build && ts-node ./run/concatSass.ts",
    "start-sass": "cd build && ts-node ./run/concatSass.ts -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-rewrite.git"
  },
  "author": "developers@rocketmakers.com",
  "keywords": [
    "API",
    "Hooks",
    "Rocketmakers",
    "React",
    "Typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@rocketmakers/shell-commands": "^1.7.1",
    "@types/ejs": "^3.0.6",
    "@types/glob": "^7.1.3",
    "chokidar": "^3.5.1",
    "ejs": "^3.1.6",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "postcss-loader": "~4.1.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-loader": "~10.1.0",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "glob": "^7.1.7",
    "sass": "^1.32.13",
    "style-loader": "^2.0.0"
  }
}
