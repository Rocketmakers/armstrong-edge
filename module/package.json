{
  "name": "@rocketmakers/armstrong-edge",
  "sideEffects": [
    "*.scss"
  ],
  "version": "0.0.0-development",
  "description": "Armstrong is a React component library made by Rocketmakers written in Typescript and SCSS.",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "copy-readme": "shx cp ../README.md .",
    "build-cjs": "tsc --pretty --extendedDiagnostics -p src/tsconfig-cjs.json -d",
    "build-esm": "tsc --pretty --extendedDiagnostics -p src/tsconfig-esm.json -d",
    "build-ts": "npm run build-cjs && npm run build-esm",
    "build": "npm run build-ts && npm run build-sass && npm run copy-readme",
    "link": "npm link",
    "test": "npm-run-all test-jest test-cypress",
    "test-jest": "jest --passWithNoTests",
    "jest-vscode-runner": "jest",
    "test-cypress": "cypress open-ct",
    "lint-ts": "eslint --max-warnings=0 \"**/*.ts{,?}\" --cache --ignore-path .eslintignore --ext ts --ext tsx",
    "lint-ts-fix": "npm run lint-ts -- --fix",
    "lint-scss": "npx stylelint \"**/*.scss\"",
    "lint-scss-fix": "npm run lint-scss -- --fix",
    "lint": "npm run lint-ts && npm run lint-scss",
    "lint-fix": "npm run lint-ts-fix && npm run lint-scss-fix",
    "prepublishOnly": "npm run lint && npm run build && npm run copy-readme",
    "build-sass": "cd build && ts-node ./run/concatSass.ts",
    "start-sass": "cd build && ts-node ./run/concatSass.ts -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release",
    "chromatic": "npx chromatic --project-token=9da5c17ef1d5"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-edge.git"
  },
  "author": "developers@rocketmakers.com",
  "keywords": [
    "API",
    "Hooks",
    "Rocketmakers",
    "React",
    "Typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "17.x || 18.x",
    "react-dom": "17.x || 18.x"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@rocketmakers/shell-commands": "^5.4.0",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addons": "6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/react": "6.5.12",
    "@storybook/test-runner": "^0.9.0",
    "@storybook/testing-react": "1.3.0",
    "@storybook/theming": "6.5.12",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/ejs": "3.0.7",
    "@types/glob": "7.1.4",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "autoprefixer": "^10.4.0",
    "babel-loader": "8.2.2",
    "chokidar": "3.5.2",
    "chromatic": "6.5.4",
    "css-loader": "5.2.6",
    "cypress": "^9.0.0",
    "cypress-localstorage-commands": "^1.6.1",
    "ejs": "3.1.6",
    "eslint": "7.30.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "file-loader": "^6.2.0",
    "glob": "7.1.7",
    "has-flag": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "local-cypress": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.1.1",
    "prettier": "2.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "sass": "1.35.2",
    "sass-loader": "^10.1.1",
    "semantic-release": "17.4.4",
    "shx": "0.3.3",
    "storybook-stylesheet-toggle": "0.1.3",
    "style-loader": "^3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6"
  },
  "dependencies": {
    "date-fns": "^2.22.1"
  }
}
