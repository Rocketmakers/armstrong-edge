{
  "name": "@rocketmakers/armstrong-edge",
  "version": "0.0.0-development",
  "description": "Armstrong is a React component library made by Rocketmakers written in Typescript and SCSS.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "copy-readme": "shx cp ../README.md .",
    "build-ts": "tsc --pretty --extendedDiagnostics -p src -d",
    "build": "npm run build-ts && npm run build-sass && npm run copy-readme",
    "link": "npm link",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-ts": "eslint --max-warnings=0 \"**/*.ts{,?}\" --cache --ignore-path .eslintignore --ext ts --ext tsx",
    "lint-ts-fix": "npm run lint-ts -- --fix",
    "lint-scss": "npx stylelint \"**/*.scss\"",
    "lint-scss-fix": "npm run lint-scss -- --fix",
    "lint": "npm run lint-ts && npm run lint-scss",
    "lint-fix": "npm run lint-ts-fix && npm run lint-scss-fix",
    "prepublishOnly": "npm run lint && npm run build && npm run copy-readme",
    "build-sass": "cd build && ts-node ./run/concatSass.ts",
    "start-sass": "cd build && ts-node ./run/concatSass.ts -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-edge.git"
  },
  "author": "developers@rocketmakers.com",
  "keywords": [
    "API",
    "Hooks",
    "Rocketmakers",
    "React",
    "Typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@rocketmakers/shell-commands": "1.7.2",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-essentials": "6.3.4",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addons": "6.3.4",
    "@storybook/builder-webpack5": "6.3.4",
    "@storybook/manager-webpack5": "6.3.4",
    "@storybook/react": "6.3.4",
    "@storybook/theming": "6.3.4",
    "@types/ejs": "3.0.7",
    "@types/glob": "7.1.4",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-loader": "8.2.2",
    "chokidar": "3.5.2",
    "css-loader": "5.2.6",
    "ejs": "3.1.6",
    "eslint": "7.30.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "glob": "7.1.7",
    "postcss-loader": "~6.1.1",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.35.2",
    "sass-loader": "~10.1.0",
    "semantic-release": "17.4.4",
    "shx": "0.3.3",
    "storybook-stylesheet-toggle": "0.1.3",
    "style-loader": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "webpack": "5.44.0"
  },
  "dependencies": {
    "date-fns": "^2.22.1"
  }
}
