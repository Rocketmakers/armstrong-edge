{
  "name": "@rocketmakers/armstrong",
  "version": "3.8.3",
  "description": "Rocketmakers React component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-edge.git"
  },
  "scripts": {
    "start": "concurrently -n armstrong- npm:watch-*",
    "copy-readme": "cp ../README.md ./README.md",
    "build": "npm run copy-readme && tsc && vite build --mode production",
    "watch-ts": "vite build --watch",
    "test": "jest",
    "test-ci": "jest --ci",
    "format": "prettier --write .",
    "lint": "eslint . --fix --quiet",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix"
  },
  "peerDependencies": {
    "date-fns": "2.x",
    "react": "18.x || 19.x",
    "react-dom": "18.x || 19.x",
    "zod": "3.*"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "manager.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs",
      "types": "./dist/*/index.d.ts"
    },
    "./css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rocketmakers/eslint": "1.0.1",
    "@rocketmakers/prettier-config": "1.0.1",
    "@rocketmakers/shell-commands": "7.1.0",
    "@rocketmakers/stylelint-config": "1.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.3",
    "@semantic-release/npm": "10.0.4",
    "@semantic-release/release-notes-generator": "11.0.4",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/test": "8.4.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.195",
    "@types/react": "19.0.1",
    "@types/react-datepicker": "4.11.2",
    "@types/react-dom": "19.0.1",
    "chokidar": "3.5.3",
    "concurrently": "8.2.0",
    "http-server": "14.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.6.1",
    "jest-config": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-junit": "16.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sass": "1.63.6",
    "scss-concat": "0.3.9",
    "semantic-release": "21.0.7",
    "storybook": "8.4.7",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.4",
    "vite-plugin-dts": "3.2.0",
    "vite-plugin-sdk": "0.1.2",
    "vite-plugin-static-copy": "0.16.0",
    "vite-plugin-turbosnap": "1.0.3",
    "wait-on": "7.0.1",
    "zod": "3.21.4"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-progress": "1.1.0",
    "@radix-ui/react-radio-group": "1.2.1",
    "@radix-ui/react-slider": "1.2.1",
    "@radix-ui/react-switch": "1.1.1",
    "@radix-ui/react-toast": "1.2.2",
    "@radix-ui/react-tooltip": "1.1.4",
    "react-datepicker": "7.6.0",
    "react-icons": "4.10.1",
    "react-select": "5.10.0"
  }
}
