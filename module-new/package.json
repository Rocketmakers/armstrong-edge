{
  "name": "@rocketmakers/armstrong-dev",
  "version": "0.0.0-development",
  "description": "Rocketmakers React component library",
  "scripts": {
    "start": "concurrently -n armstrong- npm:watch-*",
    "build": "tsc && vite build --mode production",
    "watch-ts": "vite build --watch",
    "test": "jest",
    "test-ci": "jest --ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --url http://127.0.0.1:6006",
    "test-storybook-ci": "npx concurrently -k -s first -n 'SB,TEST' -c 'magenta,blue' 'npx http-server storybook-static --port 6006 --silent' 'npx wait-on http://127.0.0.1:6006 && npm run test-storybook'",
    "format": "prettier --write .",
    "lint": "eslint . --fix --quiet",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix"
  },
  "peerDependencies": {
    "date-fns": "2.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "*.scss",
    "manager.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketmakers/eslint": "1.0.1",
    "@rocketmakers/prettier-config": "1.0.1",
    "@rocketmakers/shell-commands": "^6.1.0",
    "@rocketmakers/stylelint-config": "1.0.1",
    "@storybook/addon-a11y": "7.0.0-beta.50",
    "@storybook/addon-essentials": "7.0.0-beta.50",
    "@storybook/addon-interactions": "7.0.0-beta.50",
    "@storybook/addon-links": "7.0.0-beta.50",
    "@storybook/addons": "7.0.0-beta.50",
    "@storybook/jest": "0.0.11-next.0",
    "@storybook/react": "7.0.0-beta.50",
    "@storybook/react-vite": "7.0.0-beta.50",
    "@storybook/test-runner": "0.10.0-next.5",
    "@storybook/testing-library": "0.0.14-next.1",
    "@storybook/theming": "7.0.0-beta.50",
    "@testing-library/react": "13.4.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "chokidar": "3.5.3",
    "concurrently": "7.4.0",
    "http-server": "14.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.1",
    "jest-config": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jest-junit": "15.0.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.54.9",
    "scss-concat": "0.3.9",
    "storybook": "7.0.0-beta.50",
    "storybook-addon-designs": "^7.0.0-beta.2",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "vite": "3.1.8",
    "vite-plugin-dts": "1.4.1",
    "vite-plugin-static-copy": "0.11.0",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.3",
    "lodash": "^4.17.21",
    "react-icons": "^4.8.0",
    "react-select": "5.5.7"
  }
}
